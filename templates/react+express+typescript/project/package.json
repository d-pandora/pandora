    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "file-loader": "^4.2.0",
    "ghooks": "^2.0.4",
    "html-webpack-plugin": "^3.2.0",
    "less-loader": "^5.0.0",
    "module-alias": "^2.2.1",
    "nodemon": "^1.19.2",
    "react-hot-loader": "^4.12.14",
    "style-loader": "^1.0.0",
    "ts-import-plugin": "^1.6.1",

    "@types/multer": "^1.3.10",
    "@types/node": "^12.7.5",
    "@types/node-xlsx": "^0.12.1",
    "@types/react": "^16.9.3",
    "@types/react-dom": "^16.9.1",
    "@types/react-router": "^5.1.1",
